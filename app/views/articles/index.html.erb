<h1 id="page_title">Stuff I've written</h1>


<div id="posts_wrapper" class="skinny_wrapper">
  <% @articles.each do |article| %>
    <div class="post">
      <p class="date"><%= article.created_at.strftime("%A, %b %d") %></p>
      <h2><%= link_to article.title.titleize, article %></h2>
      <hr>
    </div>
  <% end %>

</div>

<div class="pagination">
  <span class="previous_page disabled">Prev</span>
  <em class="current">1</em>
  <a href="">2</a>
  <a href="#" class="next_page">Next</a>
</div>




































